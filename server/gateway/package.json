  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env\"",
    "start:test": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js dotenv_config_path=../.env.test\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "test": "jest --config ./tests/jest-e2e.json --detectOpenHandles"
  },
  "dependencies": {
    "@casl/ability": "^5.4.0",
    "@nestjs/common": "8.2.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/microservices": "8.2.6",
    "@nestjs/platform-express": "8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.3",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "8.2.6",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.10",
    "@types/node": "17.0.16",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "dotenv": "16.0.0",
    "eslint": "8.8.0",
    "jest": "26.4.0",
    "mongoose": "6.2.1",
    "supertest": "6.2.2",
    "ts-jest": "26.2.0",
    "ts-node": "10.5.0",
    "tsc-watch": "4.6.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  }
}
