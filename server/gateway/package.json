    "@nestjs/platform-express": "7.5.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "@nestjs/testing": "7.5.1",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.10",
    "@types/node": "14.0.27",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "jest": "26.4.0",
    "mongoose": "5.10.0",
    "supertest": "4.0.2",
    "ts-jest": "26.2.0",
    "ts-node": "9.0.0",
